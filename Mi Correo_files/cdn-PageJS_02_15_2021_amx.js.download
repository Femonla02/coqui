    	if (v_sFolder.toLowerCase() != '/drafts')
		{
			ShowLoading(true);
			
			document.getElementById("Msg_Loading").style.display = "none";
		}

    	
        var sHTTPData = o.responseText; 
        		
		// Process the JSON
		messages = [];
		//try {
				messages = YAHOO.lang.JSON.parse(sHTTPData);
