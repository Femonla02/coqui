	sMsgBody = $('#RichEditIFrameID').contents().find('body').html();
	
	
	/*
		$("body").find("*").not("a,img,br").each(function() {
    $(this).replaceWith(this.innerHTML);
	});
*/
	
	sMsgBody = sMsgBody.split('<div origmsg="yes"></div>');
