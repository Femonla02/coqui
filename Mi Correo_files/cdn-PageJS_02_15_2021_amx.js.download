	QComposer('n');
	
	$("#rte_subject").val(sSubject);
	
	/////////////////
	
		   
	if (sCc != "")
	{
		oCurrentAutoTextBox = $('#rte_cc');
		sCC = trim(sCc);
		sCC = sCC.replace(/,/g, ";")
			      
		aCcEmails = sCC.split(";");
			     
		if (aCcEmails.length >= 1)
			fnShowCcBcc('cc');
			     
	    for (i = 0; i < aCcEmails.length; i++)
	    	fnInsertRTEXContactEmail(aCcEmails[i]);
	}
			      

	if (sTo != "")
	{
		oCurrentAutoTextBox = $('#rte_to');
		sCC = trim(sTo);
		sCC = sCC.replace(/,/g, ";")
			      
		aCcEmails = sCC.split(";");
			     			     
	    for (i = 0; i < aCcEmails.length; i++)
	    	fnInsertRTEXContactEmail(aCcEmails[i]);
	}
	
	
