function fnTrim(stringToTrim) {
	return stringToTrim.replace(/^\s+|\s+$/g,"");
}
function fnLTrim(stringToTrim) {
	return stringToTrim.replace(/^\s+/,"");
}
function fnRTrim(stringToTrim) {
	return stringToTrim.replace(/\s+$/,"");
}

