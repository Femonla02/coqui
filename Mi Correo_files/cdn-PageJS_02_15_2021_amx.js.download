  	function fnGetTimestamp() 
  	{
	    dtNow = new Date();
	    var Hours = dtNow.getHours();
	    var Mins;
	    var Secs;
	    var Time;
	    if (Hours >= 12) {
	      Time = " PM";
	    } else {
	      Time = " AM";
	    }
	    if (Hours > 12) Hours -= 12;
	    if (Hours == 0) Hours = 12;
	    Mins = dtNow.getMinutes();
	    if (Mins < 10) Mins = "0" + Mins;
	    Secs = dtNow.getSeconds();
	    if (Secs < 10) Secs = "0" + Secs;
	    return (Hours + ":" + Mins + ":" + Secs + " " + Time);
  	}


