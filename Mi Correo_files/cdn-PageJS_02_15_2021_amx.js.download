{    
    if (m_iActualSpellErr > 1)
        sSpellMsg = " Misspellings Found"; 
    else
        sSpellMsg = " misspelling found";
 
 	//sMessage = m_iActualSpellErr + sSpellMsg + '</font>';
 	UpdateInboxAlert(mcLang.Text('lang_rte_69',1));

}

