function fnTOOL_JSONtoTEXT(oJSON)
{
	var items = [];
	items.push( "Total nodes:" + oJSON.length + "\n\n" );
	
	for (var i=0; i < oJSON.length; i++)
	{
		items.push( "-- node[" + i +  "]:\n" );
		
		//alert(oJSON[i]['iMemberID'])
		
		$.each( oJSON[i], function( key, val ) {
			items.push( key + ":" + val + "\n" );
		});
		
		items.push( "\n" );    	
	}

	return(items.join(""));
}


