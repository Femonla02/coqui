//:::::::
function fnPrompt(sText, sValue)
{
	if ( sValue == '' )
		sValue = ' ';

	var p=prompt(sText,sValue);
	if(!p)// cancel was *probably* clicked
	{
		return(null);
	}
	else if(p===' ')//ok was clicked with no input
	{
		return('');
	}
	else// there is user input
	{
		p = fnLTrim(p);
		return(p);
	}
	
	if ( fnTrim(p) == '' )
		return('');
	

}

