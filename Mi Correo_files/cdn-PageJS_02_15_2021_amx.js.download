	function addSubjPrefix(sPfx, s) {
	  var result = "";
	  var sLf = s.slice(0, sPfx.length);
	  if (sLf.toLowerCase() != sPfx.toLowerCase()) {
	    result = sPfx + " " + s;
	  } else {
	    result = s;
	  }
	  return (result);
	}
	 
