	//Return the translated text
	this.Text = function(sKey, bHTML)
   	{
   		if (this.bDebug)
   			return( 'XXXXX' );
   		else
   		{
   			var sValue = '';
   			if ( lang.hasOwnProperty(sKey) )
   				sValue = lang[sKey];
   			else
   				sValue = '@' +  sKey;
   				
			return( sValue );
		}
   	}
	
	//Getting the array properties' names and values
	this.GetLangKeys = function()
   	{
		var sList = '';
		var strName, strValue ;
		for(strName in lang)
		{
		   strValue = lang[strName] ;
		   sList += strName + ':' + strValue + '\n';
		}

       return(sList);
   	}
 
